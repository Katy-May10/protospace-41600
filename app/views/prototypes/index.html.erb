<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、<%= link_to current_user.name, @user_path, class: :greeting__link%>さん
      </div>
      <div class="card__wrapper">
        <%= render partial: "prototype", collection: @prototypes %>
      </div>
    <% else %>
      <div class="card__wrapper">
        <%= render partial: "prototype", collection: @prototypes %>
      </div>
    <% end %>
  </div>
</main>
